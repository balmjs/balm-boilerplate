{
  "name": "balm-boilerplate",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "lodash": "^4.14.0",
    "modernizr": "~2.8.3",
    "normalize-css": "^5.0.0"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    },
    "font-awesome": {
      "main": [
        "fonts/*",
        "less/font-awesome.less",
        "scss/font-awesome.scss"
      ]
    }
  }
}
