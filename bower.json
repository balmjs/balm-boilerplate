{
  "name": "balm-demo-boilerplate",
  "private": true,
  "dependencies": {
    "bootstrap": "3.4.1",
    "bootstrap-sass": "3.4.1",
    "font-awesome": "4.7.0",
    "jquery": "^3.0.0",
    "lodash": "^4.0.0",
    "normalize-css": "^8.0.1"
  },
  "overrides": {
    "bootstrap": {
      "main": ["less/bootstrap.less", "dist/js/bootstrap.js", "fonts/*"]
    },
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/javascripts/bootstrap.js",
        "assets/fonts/bootstrap/*"
      ]
    },
    "font-awesome": {
      "main": ["less/font-awesome.less", "scss/font-awesome.scss", "fonts/*"]
    }
  }
}
